            "examples": {
              "application/json": "{\n  \"data\":{\n      \"type\": \"TapGwEndpoint\",\n      \"id\": \"XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXX\",\n      \"attributes\":{\n          \"participantId\": \"urn:oasis:names:tc:ebcore:partyid-type:iso6523:0151::9999999999\",\n          \"endpointType\": \"ausdigital-tap/2\",\n          \"keyX\": \"valueX\"\n      }\n  }\n}"
