          },
          "500": {
            "$ref": "#/responses/500"
          }
        }
      },
      "patch": {
        "tags": [
          "Endpoint Details"
        ],
        "summary": "Endpoint Update",
        "description": "Update only passed set of keys/values. Only endpoints for the given user could be updated. New version of the object is returned, with metadata updated.",
        "operationId": "patchEndpointDetails",
        "parameters": [
          {
            "$ref": "#/parameters/contentTypeHeader"
          },
          {
            "$ref": "#/parameters/endpointId"
          },
          {
            "description": "The TAP Endpoint metadata body parameter.",
            "in": "body",
            "name": "body",
            "required": true,
            "schema": {
              "type": "object",
              "example": {
                "keyX": "ValueX"
              }
            }
